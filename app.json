{
  "expo": {
    "name": "apployMe",
    "slug": "apployMe",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.3",
    "orientation": "portrait",
    "icon": "./ios/assets/app_icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#573EC1"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 45000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.apployme",
      "buildNumber": "5"
    },
    "android": {
      "package": "com.apployme",
      "versionCode": 1
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}